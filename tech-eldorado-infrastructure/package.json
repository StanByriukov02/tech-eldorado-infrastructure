{
  "name": "tech-eldorado-infrastructure",
  "version": "1.0.0",
  "description": "Tech Eldorado - Complete Multi-Agent Infrastructure",
  "type": "module",
  "scripts": {
    "dev": "node --watch backend/server.js",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "echo 'Backend build'",
    "build:frontend": "cd frontend && npm run build",
    "start": "node backend/server.js",
    "migrate": "node backend/db/migrate.js",
    "seed": "node backend/db/seed.js",
    "check:departments": "node scripts/check-departments.js",
    "db:setup": "npm run migrate && npm run seed",
    "setup:supabase": "echo 'See setup/STEP_BY_STEP_SUPABASE.md for instructions'",
    "test:connection": "node test-connection.js",
    "register:ceo": "node scripts/register-ceo.js",
    "register:ceo:auto": "node scripts/register-ceo-auto.js",
    "test:ceo": "node scripts/test-ceo-login.js",
    "test:ceo:auto": "node scripts/test-ceo-login-auto.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.14.2",
    "drizzle-orm": "^0.29.0",
    "postgres": "^3.4.3",
    "@supabase/supabase-js": "^2.39.0",
    "redis": "^4.6.11",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/ws": "^8.5.10",
    "@types/node": "^20.10.5",
    "typescript": "^5.3.3",
    "drizzle-kit": "^0.20.6"
  }
}

